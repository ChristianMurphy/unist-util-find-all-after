{
  "name": "unist-util-find-all-after",
  "version": "1.0.0",
  "description": "Utility to find nodes after another node",
  "license": "MIT",
  "keywords": [
    "unist",
    "mdast",
    "markdown",
    "retext",
    "natural",
    "language",
    "node",
    "find",
    "after",
    "util",
    "utility"
  ],
  "dependencies": {
    "unist-util-is": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/unist-util-find-all-after.git"
  },
  "author": {
    "name": "Titus Wormer",
    "email": "tituswormer@gmail.com"
  },
  "files": [
    "index.js",
    "LICENSE"
  ],
  "devDependencies": {
    "browserify": "^13.0.0",
    "esmangle": "^1.0.0",
    "remark": "^6.0.0",
    "remark-cli": "^2.0.0",
    "remark-preset-wooorm": "^1.0.0",
    "nyc": "^9.0.1",
    "tape": "^4.6.2",
    "xo": "^0.17.1"
  },
  "scripts": {
    "bundle": "browserify index.js --no-builtins -s unistUtilFindAllAfter > unist-util-find-all-after.js",
    "postbundle": "esmangle unist-util-find-all-after.js > unist-util-find-all-after.min.js",
    "build-md": "remark . -foq",
    "build": "npm run bundle && npm run build-md",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "ignore": [
      "unist-util-find-all-after.js"
    ]
  },
  "remarkConfig": {
    "presets": "wooorm"
  }
}
